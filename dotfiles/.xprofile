#!/bin/bash
#
# Profile for X11
# Gets run by DMs

# Set X keymap because reasons
setxkbmap ch

# Start applications
{%@@ if profile == "sun" @@%}
launch xrdb compton bspwm sxhkd polybar enpass udiskie color ibus hp
{%@@ elif profile == "teegardenb" or profile == "kepler22b" @@%}
launch xrdb compton bspwm sxhkd polybar enpass udiskie color ibus
{%@@ endif @@%}
nm-applet &
